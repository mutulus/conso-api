{% extends 'base.html.twig' %}

{% block title %}Formulaire de connexion{% endblock %}

{% block body %}
    <h1 class="mt-5 text-center">Se connecter</h1>
    {{ form_start(form) }}
    {{ form_row(form.mail) }}
    {{ form_row(form.pass) }}
    <button type="submit" class="btn btn-primary">Valider</button>
    {{ form_end(form) }}

{% endblock %}
